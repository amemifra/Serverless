{
  "extends": "airbnb-base",

  "rules": {
    "comma-dangle": 0,
    "arrow-parens": ["error", "as-needed"],
    "no-unused-expressions": 0,
    "import/prefer-default-export": 0,
    "no-prototype-builtins": 1,
    "no-unused-vars": 0,
    "no-restricted-syntax": 0,
    "prefer-template": 1,
    "max-len": 0,
    "no-confusing-arrow": 1,
    "linebreak-style": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}
